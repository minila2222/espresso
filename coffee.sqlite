query = QSqlQuery()
        query.exec("""
        CREATE TABLE IF NOT EXISTS coffee (
            id INTEGER PRIMARY KEY AUTOINCREMENT,
            name TEXT NOT NULL,
            roast_level TEXT NOT NULL,
            ground_or_bean TEXT NOT NULL,
            taste_description TEXT,
            price REAL,
            package_size REAL
        )
        """)

        query.exec("SELECT COUNT(*) FROM coffee")
        query.next()
        if query.value(0) == 0:
            query.exec("""
            INSERT INTO coffee (name, roast_level, ground_or_bean, taste_description, price, package_size)
